<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <base href="<%=basePath%>">
    
    <title>My JSP 'Training.jsp' starting page</title>
    
	<meta http-equiv="pragma" content="no-cache">
	<meta http-equiv="cache-control" content="no-cache">
	<meta http-equiv="expires" content="0">    
	<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
	<meta http-equiv="description" content="This is my page">
	<!--
	<link rel="stylesheet" type="text/css" href="styles.css">
	-->
<link href="../css/bootstrap.min.css" rel="stylesheet" />
    <link href="../css/Eworks.css" rel="stylesheet" />



    <script src="../js/jquery-1.8.3.min.js"></script>

    <link href="../control/css/zyUpload.css" rel="stylesheet" />
    <script src="../control/js/zyUpload.js"></script>
    <script src="../js/demo.js"></script>
    <script src="../core/zyFile.js"></script>
  </head>
<body>
    <div class="container">
        <div class="header">
            <div class="title">
                <span>e创中心</span>
            </div>
        </div>
        <div class="e_works_con">
            <div>
                <label>标题：</label><input type="text" class="input" />
            </div>
            <div class="clear address">
                <label class="e_works_con_label">所在区域：</label>
                <div style="float:left;width:50%;">
                    <select id="Select1">

                        <option>选择省份</option>
                    </select>&nbsp;&nbsp;&nbsp;
                    <select id="Select2">
                        <option>选择城市</option>

                    </select>&nbsp;&nbsp;&nbsp;
                    <select id="Select3">
                        <option>选择区域</option>

                    </select>
                </div>
            </div>

            <div class="clear">
                <label>详细地址：</label>
                <input type="text" class="input" />
            </div>
            <div class="clear">
                <label class="e_works_con_label">类型：</label>
                <div style="float:left; width:50%; margin-top:0px;">
                    <ul class="gq">
                        <li><input type="radio" name="supply" />创业培训</li>
                        <li><input type="radio" name="supply" />管理培训</li>
                        <li><input type="radio" name="supply" />技术培训</li>
                        <li><input type="radio" name="supply" />其他</li>
                    </ul>
                </div>
            </div>
            <div class="clear">
                <label>价格：</label>
                <input type="text" class="insert" />
            </div>

            <div>
                <label class="label_align" style="float:left">培训展示：</label>
                <div id="show" class="show"></div>
            </div>


            <div class="clear">
                <label class="label_align" style="margin-top:20px;">详细说明：</label> <textarea style="margin-top:20px;"></textarea>
            </div>
            <div>
                <label>电话：</label> <input type="text" class="insert" />
            </div>
            <div>
                <label>联系人：</label> <input type="text" class="insert" />
            </div>
            <div>
                <label></label>
                <input type="button" value="确认发布" class="submit" />
            </div>


        </div>
    </div>
   
</body>
</html>
